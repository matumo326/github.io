        <value name="lightness">
        <shadow type="math_number">
          <field name="NUM">50.0</field>
        </shadow>
        </value>
      </block>

      <block type="neopixel_rotate_right">
       <value name="step">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
       </value>
      </block>

      <block type="neopixel_rotate_left">
       <value name="step">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
       </value>
