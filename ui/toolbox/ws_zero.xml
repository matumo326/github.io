  <category name="イルミパターン" colour="40">
      <block type="pattern_huwa">
       <value name="color">
        <block type="neopixel_color_numbers">
         <value name="red">
          <shadow type="math_number">
            <field name="NUM">100</field>
          </shadow>
         </value>
         <value name="green">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
         <value name="blue">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
        </block>
       </value>
       <value name="speed">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
      </block>

      <block type="pattern_random_blink">
       <value name="num">
        <shadow type="math_number">
          <field name="NUM">20</field>
        </shadow>
       </value>
       <value name="speed">
        <shadow type="math_number">
          <field name="NUM">3</field>
        </shadow>
       </value>
       <value name="count">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
      </block>

      <block type="pattern_change_move_up">
       <value name="speed">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
       <value name="duration">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
      </block>

      <block type="pattern_change_move_down">
       <value name="speed">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
       <value name="duration">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
       </value>
      </block>

  </category>

