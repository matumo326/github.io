       <value name="color">
        <block type="neopixel_color_numbers">
         <value name="red">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
         <value name="green">
          <shadow type="math_number">
            <field name="NUM">0</field>
