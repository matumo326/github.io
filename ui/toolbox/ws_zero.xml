            <field name="NUM">0</field>
          </shadow>
         </value>
        </block>
       </value>
      </block>

      <block type="neopixel_control_all">
       <value name="color">
        <block type="neopixel_color_numbers">
         <value name="red">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
         <value name="green">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
         <value name="blue">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
         </value>
        </block>
       </value>
      </block>
      <block type="neopixel_color_numbers">
       <value name="red">
        <shadow type="math_number">
          <field name="NUM">20</field>
